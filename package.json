{
  "name": "kuzzle-enterprise-probe",
  "version": "0.0.1",
  "description": "Kuzzle Enterprise plugin: statistical data probe",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuzzleio/kuzzle-enterprise-probe.git"
  },
  "scripts": {
    "unit-testing": "istanbul cover _mocha",
    "test": "npm run --silent lint && npm run unit-testing --coverage",
    "lint": "eslint --max-warnings=0 ./lib ./test"
  },
  "keywords": [
    "kuzzle",
    "enterprise",
    "plugin",
    "worker",
    "probe"
  ],
  "author": {
    "name": "Kuzzle Team"
  },
  "bugs": {
    "url": "https://github.com/kuzzleio/kuzzle-enterprise-probe/issues"
  },
  "pluginInfo": {
    "defaultConfig": {
      "threads": 1,
      "loadedBy": "server",
      "maxMemoryRestart": "1G",
      "databases": [
        "localhost:9200"
      ],
      "storageIndex": "measures",
      "probes": {}
    }
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "bluebird": "^3.4.6",
    "elasticsearch": "^11.0.1",
    "lodash": "^4.12.0",
    "long-timeout": "^0.1.1",
    "ms": "^0.7.1",
    "q": "^1.4.1",
    "random-js": "^1.0.8"
  },
  "devDependencies": {
    "eslint": "^2.9.0",
    "istanbul": "^0.4.3",
    "lolex": "^1.5.0",
    "mocha": "^2.4.5",
    "proxyquire": "^1.7.9",
    "should": "^8.3.1",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.0"
  },
  "maintainers": [
    {
      "name": "kuzzle",
      "email": "support@kuzzle.io"
    }
  ],
  "engines": {
    "node": ">= 4.3.1",
    "npm": ">= 2.14.4"
  }
}
